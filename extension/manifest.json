{
    "manifest_version": 2,
    "name": "LinkedIn browser plugin",
    "description": "Color job description pages on LinkedIn",
    "version": "1.1",
    "content_scripts": [
        {
            "matches": [
                "https://www.linkedin.com/jobs/search/*"
            ],
            "js": [
                "index.js"
            ]
        }
    ],
    "permissions": [
        "activeTab"
    ]
}
